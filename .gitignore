# Ignore configuration files that may contain sensitive information.
sites/**/settings*.php
sites/**/services*.yml

# Ignore files and private directories.
sites/**/files
sites/**/private

# Ignore directories related to local development environments.
/.idea/
/.vscode/
/.DS_Store
/_ide_helper.php
/.phpstorm.meta.php

# Ignore common files that are usually specific to a developer's machine.
Thumbs.db
*.bak
*.swp
*.tmp

# Ignore dependencies managed by Composer.
/vendor/
/web/core
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore SimpleTest multi-site environment.
/sites/simpletest

# Ignore files generated by PhpStorm.
*.iml
.idea/
.idea/*.*

# Ignore Sass cache files.
.sass-cache/
